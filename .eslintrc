{
  "extends": ["airbnb/base", "react-app", "react-app/jest"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "no-plusplus": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "quotes": "off",
    "eol-last": "off",
    "max-len": ["error", 150],
    "arrow-body-style": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "no-multiple-empty-lines": "off",
    "no-trailing-spaces": "off",
    "padded-blocks": "off",
    "no-shadow": "off"
  }
}